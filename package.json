    "prepare": "husky install && npm run fix:sass"
  },
  "dependencies": {
    "@unocss/reset": "^0.46.2",
    "@vueuse/core": "^9.0.0",
    "core-js": "^3.24.0",
    "element-ui": "^2.15.10",
    "vue": "^2.7.8",
    "vue-router": "^3.5.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.2.0",
    "@iconify/json": "^2.1.83",
    "@oit/eslint-config-vue-ts": "^0.4.3",
    "@vitejs/plugin-vue2": "^1.1.2",
    "commitlint": "^17.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass": "^1.55.0",
    "sass-migrator": "^1.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.46.2",
    "unplugin-auto-import": "^0.10.1",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.3",
    "vite-plugin-eslint": "^1.8.1",
    "vue-template-compiler": "^2.7.8",
    "vue-template-es2015-compiler": "^1.9.1"
